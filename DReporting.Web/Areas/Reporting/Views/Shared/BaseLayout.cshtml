@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <script type="text/javascript" src="@Url.Content("~/bundles/js/reporting")"></script>
    @RenderSection("scripts", required: false)
    <link type="text/css" rel="stylesheet" href="@Url.Content("~/bundles/css/reporting")" />
    @RenderSection("styles", required: false)
</head>
<body>
    <div>
        @RenderBody()
        @RenderSection("body", required: false)
    </div>
</body>
</html>
